{
  "name": "ts-express-typeorm",
  "version": "1.4.2",
  "description": "typescript + express.js + typeorm",
  "author": "Sangdon Park <pravusid@gmail.com>",
  "main": "dist/main.js",
  "scripts": {
    "ci:prod": "npm ci --only=prod",
    "prestart": "npm run clean && npm run build",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "npm run build && concurrently -p none \"tsc -w\" \"nodemon --watch dist --exec 'node -r source-map-support/register' dist/main.js\"",
    "dev:ts": "nodemon --watch src --exec 'ts-node' src/main.ts",
    "dev:kill": "pgrep -fl nodemon | awk '{print $1}' | xargs -r -I{} kill -2 {} && lsof -ti :8080 | xargs -r -I{} kill {}",
    "typecheck": "tsc --noEmit --diagnostics --incremental false",
    "circular": "npm run prestart && madge --circular dist/",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint --fix 'src/**/*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/pino": "^6.3.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "pino": "^6.5.0",
    "pino-pretty": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "jest": "^26.2.2",
    "madge": "^3.9.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2"
  }
}
