{
  "name": "ts-express-typeorm",
  "version": "1.2.1",
  "description": "typescript + express.js + typeorm",
  "author": "Sangdon Park <pravusid@gmail.com>",
  "main": "dist/main.js",
  "scripts": {
    "ci:prod": "npm ci --only=prod",
    "prestart": "npm run clean && npm run build",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "tsc",
    "clean": "rimraf tsconfig.tsbuildinfo && rimraf dist",
    "dev": "npm run build && concurrently -p none \"tsc -w\" \"nodemon --watch dist --exec 'node -r source-map-support/register' dist/main.js\"",
    "dev:ts": "nodemon --watch src --exec 'ORM_ENV=ts ts-node' src/main.ts",
    "typecheck": "tsc --noEmit --diagnostics --incremental false",
    "circular": "npm run prestart && madge --circular dist/",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint --fix 'src/**/*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.4",
    "@types/pino": "^5.15.5",
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "pino": "^5.16.0",
    "pino-pretty": "^3.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.22",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@types/faker": "^4.1.9",
    "@types/jest": "^25.1.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "madge": "^3.8.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2"
  }
}
