{
  "name": "ts-express-typeorm",
  "version": "1.5.1",
  "description": "typescript + express.js + typeorm",
  "author": "Sangdon Park",
  "main": "dist/main.js",
  "scripts": {
    "ci:prod": "npm ci --only=prod",
    "prestart": "npm run clean && npm run build",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "npm run build && concurrently -p none \"tsc -w\" \"nodemon --watch dist --exec 'node -r source-map-support/register' dist/main.js\"",
    "dev:ts": "nodemon --watch src --exec 'ts-node' src/main.ts",
    "dev:kill": "pgrep -fl nodemon | awk '{print $1}' | xargs -r -I{} kill -2 {} && lsof -ti :8080 | xargs -r -I{} kill {}",
    "typecheck": "tsc --noEmit --diagnostics --incremental false --tsBuildInfoFile null",
    "circular": "npm run prestart && madge --circular dist/",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint --fix 'src/**/*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.41",
    "@types/pino": "^6.3.8",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "pino": "^6.11.3",
    "pino-pretty": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "madge": "^4.0.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1"
  }
}
