{
  "name": "ts-express-typeorm",
  "version": "1.5.4",
  "description": "typescript + express.js + typeorm",
  "author": "Sangdon Park",
  "main": "dist/main.js",
  "scripts": {
    "prestart": "pnpm run clean && pnpm run build",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "pnpm run build && concurrently -p none \"tsc -w\" \"nodemon --watch dist --exec 'node -r source-map-support/register' dist/main.js\"",
    "dev:ts": "nodemon --watch src --exec 'ts-node' src/main.ts",
    "dev:kill": "pgrep -fl $(pwd)/node_modules/.bin | awk '{print $1}' | xargs -r -I{} kill -2 {} && lsof -ti :8080 | xargs -r -I{} kill {}",
    "typecheck": "tsc --noEmit --incremental false --tsBuildInfoFile null",
    "circular": "pnpm run prestart && madge --circular dist/",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint --fix 'src/**/*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.10",
    "@types/pino": "^6.3.11",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.0",
    "pino": "^6.13.1",
    "pino-pretty": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^2.0.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "madge": "^5.0.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1"
  }
}
