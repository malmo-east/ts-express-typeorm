{
  "name": "ts-express-typeorm",
  "version": "1.6.0",
  "description": "typescript + express.js + typeorm",
  "author": "Sangdon Park",
  "main": "dist/main.js",
  "scripts": {
    "prestart": "pnpm run clean && pnpm run build",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "pnpm run build && concurrently -p none \"tsc -w\" \"nodemon --watch dist --exec 'node -r source-map-support/register' dist/main.js | pino-pretty\"",
    "dev:ts": "nodemon --watch src --exec 'ts-node' src/main.ts | pino-pretty",
    "dev:kill": "pgrep -fl $(pwd)/node_modules/.bin | awk '{print $1}' | xargs -r -I{} kill -2 {} && lsof -ti :8080 | xargs -r -I{} kill {}",
    "dev:debug": "pnpm run build && concurrently -p none \"tsc -w\" \"nodemon --watch dist --exec 'DEBUG=1 node -r source-map-support/register --inspect=9229' dist/main.js | pino-pretty\"",
    "typecheck": "tsc --noEmit --incremental false --tsBuildInfoFile null",
    "circular": "pnpm run prestart && madge --circular dist/",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint --fix 'src/**/*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.3",
    "body-parser": "^1.20.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "pino": "^8.7.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.1",
    "madge": "^5.0.1",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  }
}
