{
  "name": "express-ts-sample",
  "version": "0.4.0",
  "description": "typescript + express.js + typeorm",
  "author": "Sangdon Park <pravusid@gmail.com>",
  "main": "dist/main.js",
  "scripts": {
    "prestart": "npm run clean && npm run build",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "tsc",
    "clean": "rimraf tsconfig.tsbuildinfo && rimraf dist",
    "dev": "npm run build && concurrently -p none 'tsc -w' 'nodemon --watch dist --exec \"NODE_ENV=development node -r source-map-support/register\" dist/main.js'",
    "dev:ts": "nodemon --watch src --exec \"NODE_ENV=debug ts-node\" src/main.ts",
    "debug": "nodemon --watch src --exec \"NODE_ENV=debug node --inspect-brk -r ts-node/register\" src/main.ts",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint --fix 'src/**/*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.16",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.4",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.3"
  }
}
